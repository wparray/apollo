{
	"name": "ThemeKit",
	"version": "1.0.0",
	"description": "The simplest WordPress starter theme including full setup for Sass, PostCSS, Autoprefixer, stylelint, Webpack, ESLint, imagemin, Browsersync, etc.",
	"scripts": {
		"dev": "mix watch --mix-config=./.dev/webpack.mix.js",
		"build": "mix  --mix-config=./.dev/webpack.mix.js",
		"build:production": "mix --production  --mix-config=./.dev/webpack.mix.js"
	},
	"license": "GPL-2.0+",
	"devDependencies": {
		"@tailwindcss/line-clamp": "^0.4.4",
		"autoprefixer": "^10.4.2",
		"browser-sync": "^2.27.7",
		"browser-sync-webpack-plugin": "^2.3.0",
		"copy-webpack-plugin": "^8.1.1",
		"laravel-mix": "^6.0.49",
		"laravel-mix-copy-watched": "^2.3.1",
		"laravel-mix-glob": "^2.0.2",
		"mix-tailwindcss": "^1.3.0",
		"postcss": "^8.2.15",
		"postcss-import": "^14.0.2",
		"postcss-nested": "^5.0.5",
		"postcss-nested-ancestors": "^2.0.0",
		"prettier": "^2.3.0",
		"resolve-url-loader": "^4.0.0",
		"sass": "^1.32.13",
		"sass-loader": "^11.1.0",
		"stylelint": "^13.13.1",
		"stylelint-config-standard": "^22.0.0",
		"stylelint-webpack-plugin": "^3.0.1",
		"tailwindcss": "^3.3.3",
		"webpack": "^5.68.0",
		"webpack-cli": "^5.1.4",
		"webpack-livereload-plugin": "^3.0.2"
	},
	"dependencies": {
		"@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
		"postcss-nesting": "^12.1.2"
	}
